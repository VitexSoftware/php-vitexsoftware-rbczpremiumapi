{
    {{#composerPackageName}}
    "name": "{{.}}",
    {{/composerPackageName}}
    {{#artifactVersion}}
    {{/artifactVersion}}
    "description": "{{{appDescription}}}",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "raiffeisenbank"
    ],
    "homepage": "https://github.com/VitexSoftware/php-vitexsoftware-raiffeisenbank",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "homepage": "https://vitexsoftware.com"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.10",
        "guzzlehttp/psr7": "^2.8.0",
        "vitexsoftware/ease-core": "^1.49"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.6",
        "friendsofphp/php-cs-fixer": "^3.88",
        "ergebnis/composer-normalize": "^2.48",
        "ergebnis/php-cs-fixer-config": "^6.55",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "{{escapedInvokerPackage}}\\" : "{{srcBasePath}}/",
            "{{escapedInvokerPackage}}\\Model\\" : "{{srcBasePath}}/Model",
            "{{escapedInvokerPackage}}\\PremiumAPI\\" : "{{srcBasePath}}/PremiumAPI"
	}
    },
    "autoload-dev": {
        "psr-4": {
	    "{{escapedInvokerPackage}}\\Test\\" : "{{testBasePath}}/",
            "{{escapedInvokerPackage}}\\Test\\Model\\" : "{{testBasePath}}/Model",
            "{{escapedInvokerPackage}}\\Test\\PremiumAPI\\" : "{{testBasePath}}/PremiumAPI"
	}
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }

}
